<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="16dp">

        <Button
            android:id="@+id/btn_goto_setting"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="设置页面" />

        <Button
            android:id="@+id/btn_goto_ros2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ros控制页面" />

        <Button
            android:id="@+id/btn_goto_task"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ros任务页面" />

        <Button
            android:id="@+id/btn_goto_position"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ros点位页面" />

        <Button
            android:id="@+id/btn_goto_map"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ros地图页面" />

        <Button
            android:id="@+id/begin_work"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ros开启定时巡航" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>